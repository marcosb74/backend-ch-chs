      <div class="container">
        <table class="table table-dark">
          <thead>
            <tr>
              <th scope="col">Name</th>
              <th scope="col">Price</th>
              <th scope="col">Thumbnail</th>
            </tr>
          </thead>
          <tbody>
            <tr>{{#each data}}
              <td>{{this.title}}</td>
              <td>{{this.price}}</td>
              <td><img src={{this.thumbnail}} alt={{this.title}} width="100" height="100"/></td>
            </tr>
              {{/each}}
        </table>
      </div>
